{
  "name": "aframe-redux-component",
  "version": "0.1.0",
  "description": "Hook in Redux reducers, data bindings, and action dispatches for A-Frame",
  "main": "index.js",
  "scripts": {
    "build": "browserify examples/main.js -o examples/build.js",
    "dev": "budo examples/main.js:build.js --dir examples --port 8000 --live --open",
    "dist": "webpack index.js dist/aframe-redux-component.js && webpack -p index.js dist/aframe-redux-component.min.js",
    "lint": "semistandard -v | snazzy",
    "postpublish": "npm run dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ngokevin/kframe.git"
  },
  "keywords": [
    "aframe",
    "aframe-component",
    "aframe-vr",
    "vr",
    "mozvr",
    "webvr"
  ],
  "author": "Kevin Ngo <me@ngokevin.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ngokevin/kframe/issues"
  },
  "homepage": "https://github.com/ngokevin/kframe/tree/components/redux#readme",
  "devDependencies": {
    "aframe": "aframevr/aframe#master",
    "browserify": "^13.0.0",
    "budo": "^8.2.2",
    "inquirer": "^1.0.2",
    "randomcolor": "^0.4.4",
    "semistandard": "^8.0.0",
    "shelljs": "^0.7.0",
    "shx": "^0.1.1",
    "snazzy": "^4.0.0",
    "webpack": "^1.13.0"
  },
  "semistandard": {
    "ignore": [
      "examples/build.js",
      "dist/**"
    ]
  },
  "dependencies": {
    "redux": "^3.5.2"
  }
}
